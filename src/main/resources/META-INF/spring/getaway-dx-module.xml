<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean class="org.jahia.modules.getaway.servlet.GetAwayFilter">
        <property name="urlPatterns" value="/getaway" />
        <property name="corsOrigin" value="${getaway.config.cors.origin:http://localhost:3000}" />
    </bean>

    <bean class="org.jahia.modules.getaway.content.nodetypes.initializers.LandmarksChoicelistInitializer" init-method="fillMock">
        <property name="key" value="landmarks" />
    </bean>

</beans>